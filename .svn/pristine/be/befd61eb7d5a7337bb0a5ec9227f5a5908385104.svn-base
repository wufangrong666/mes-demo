import request from '@/utils/request'
//列表查询
export function bjdQueryPageList(data) {
    return request({
        url: 'bjd/queryPageList',
        method: 'post',
        data: data,
        headers: {
            'Content-Type': 'application/json;charset=UTF-8'
        }
    })
}
