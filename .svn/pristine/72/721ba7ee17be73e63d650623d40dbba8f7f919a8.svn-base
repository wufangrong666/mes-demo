// 预收款单查询列表
export function yskdQueryPageList(data) {
    return request({
        url: 'yskd/queryPageList',
        method: 'post',
        data: data,
        headers: {
            'Content-Type': 'application/json;charset=UTF-8'
        }
    })
}
