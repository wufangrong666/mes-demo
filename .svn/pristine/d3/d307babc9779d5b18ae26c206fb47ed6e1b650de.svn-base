import request from '@/utils/request'
// 单号规则查询
export function dhgzQueryDhgz(data) {
    return request({
        url: 'dhgz/queryDhgz',
        method: 'post',
        data: data
    })
}
// 单号规则删除
export function dhgzDeleteById(data) {
    return request({
        url: 'dhgz/deleteById',
        method: 'post',
        data: data
    })
}
// 单号规则新增
export function dhgzAddAndUpdate(data) {
    return request({
        url: 'dhgz/addAndUpdate',
        method: 'post',
        data: data,
        headers: {
            'Content-Type': 'multipart/form-data'
        }
    })
}
