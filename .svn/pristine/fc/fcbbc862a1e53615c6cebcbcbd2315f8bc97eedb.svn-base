        .then((res) => {
          this.dwttList = res.data;
          this.from.dwtt = res.data[0].id;
          this.querySl();
        })
        .catch((res) => {});
      cgddFindCgddInfoByCgddId({
        cgddId: this.$route.query.id,
      })
