import request from '@/utils/request'
// 查询图片
export function logoQueryLogo(data) {
    return request({
        url: 'logo/queryLogo',
        method: 'post',
        data: data
    })
}
// 修改上传logo设置
export function logoUploadLogo(data) {
    return request({
        url: 'logo/uploadLogo',
        method: 'post',
        data: data,
        headers: {
            'Content-Type': 'multipart/form-data'
        }
    })
}

