        if (this.tableList[i].quantity == "") {
          this.msgInfo("请输入数量!");
          return;
        }
        if (this.tableList[i].quantity <= 0) {
          this.msgInfo("请输入有效数量!");
          return;
        }
        console.log(this.tableList[i].hsdj);
        if (this.tableList[i].hsdj == "" || this.tableList[i].hsdj == null) {
          this.msgInfo("请输入含税单价!");
          return;
        }
      }
      this.from.zkhZje = this.from.zje * (this.from.zdzk / 100);
      if (this.wsdjCheckbox) {
        this.from.pricelx = 1;
