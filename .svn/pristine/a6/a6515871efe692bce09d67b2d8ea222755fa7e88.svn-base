    deletClick(val) {
      this.tableList.splice(val, 1);
    },
    receiveMessage(event) {
      if (event.data.type == "table") {
        this.widowData.type = "bjdXtui";
        this.widowData.id = this.encode(event.data.id);
        this.queryData();
      }
    },
  },

  created() {},

  mounted() {
    setTimeout(() => {
      this.width = 100 / $(".el-table__row")[0].cells.length + "%";
      this.widthList = $(".el-table__row")[0].cells.length;
    }, 300);
    this.widowData = this.$route.query;
    if (this.widowData.type == "xjdXtui") {
      this.widowData.id = JSON.parse(this.$route.query.list)
        .map((item) => {
          return item.xjdmxId;
        })
        .join(",");
    }
    this.creator = Lockr.get("loginUserInfo").realname;
    window.addEventListener("message", this.receiveMessage, false);
    this.createDate = this.getTime();
    this.queryData();
  },
};
