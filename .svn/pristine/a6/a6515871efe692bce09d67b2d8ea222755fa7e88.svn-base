                message: "该单号已占用,已为你生成新单号" + res.xsddDh,
                type: "success",
                onClose: () => {
                  window.opener.postMessage("child", window.location);
                  window.top.close();
                  this.loading = false;
                },
              });
            } else {
              this.loading = true;
              this.$message({
                message: "新增成功",
                type: "success",
                onClose: () => {
                  window.opener.postMessage("child", window.location);
                  window.top.close();
                  this.loading = false;
                },
              });
            }
          } else if (this.widowData.type == "edit") {
            this.loading = true;
            this.$message({
              message: "编辑成功",
              type: "success",
              onClose: () => {
                window.opener.postMessage("child", window.location);
