import request from '@/utils/request'
//列表查询
export function crmGysQueryPageList(data) {
    return request({
        url: 'crmGys/queryPageList',
        method: 'post',
        data: data,
        headers: {
            'Content-Type': 'application/json;charset=UTF-8'
        }
    })
}
